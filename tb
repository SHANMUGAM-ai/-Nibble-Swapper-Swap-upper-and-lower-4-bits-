module tb_nibble_swapper;
    reg [7:0] in_byte;
    wire [7:0] out_byte;

    nibble_swapper uut (.in_byte(in_byte), .out_byte(out_byte));

    initial begin
        $dumpfile("nibble_swapper.vcd"); $dumpvars(0, tb_nibble_swapper);
        in_byte = 8'hAB; #10;
        in_byte = 8'h3C; #10;
        in_byte = 8'hF0; #10;
        $finish;
    end
endmodule
